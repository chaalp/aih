{
  "name": "flu_outbreak",
  "remarks": [
    "Simulates a flu-like illness with clustering symptoms using valid logic."
  ],
  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "Flu Entry Chance"
    },
    "Flu Entry Chance": {
      "type": "DistributedTransition",
      "distributions": [
        {
          "transition": "Flu Symptoms",
          "distribution": 0.25
        },
        {
          "transition": "End",
          "distribution": 0.75
        }
      ]
    },
    "Flu Symptoms": {
      "type": "Symptom",
      "symptom": "Fever",
      "severity": 80,
      "direct_transition": "Cough Symptom"
    },
    "Cough Symptom": {
      "type": "Symptom",
      "symptom": "Cough",
      "severity": 70,
      "direct_transition": "Fatigue Symptom"
    },
    "Fatigue Symptom": {
      "type": "Symptom",
      "symptom": "Fatigue",
      "severity": 60,
      "direct_transition": "Flu Diagnosis"
    },
    "Flu Diagnosis": {
      "type": "ConditionOnset",
      "codes": [
        {
          "system": "ICD-10",
          "code": "J10",
          "display": "Influenza due to identified influenza virus"
        }
      ],
      "direct_transition": "End"
    },
    "End": {
      "type": "Terminal"
    }
  }
}